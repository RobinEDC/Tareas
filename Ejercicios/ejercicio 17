# Ejercicio 17
suma = 0
contador = 0

while True:
    numero = int(input("Ingrese un número (0 para terminar): "))
    if numero == 0:
        break
    if numero % 10 == 5:
        suma += numero
        contador += 1

if contador > 0:
    promedio = suma / contador
    print("El promedio de los números terminados en 5 es:", promedio)
else:
    print("No se ingresaron números terminados en 5.")
